public class Main {
	public static void main(String[] args) {
		int number = 136689;
		boolean result;
		CheckNumber n1 = new CheckNumber();
		result = n1.checkNumber(number);
		if(result) {
			System.out.println("Not an increasing number");
		}else {
			System.out.println("it is an increasing number");
		}
	}

}
