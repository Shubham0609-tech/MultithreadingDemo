package com.cg.labs.exercise5;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		String string = scanner.next();
		
		PositiveString checker = new PositiveString();
		
		boolean result = checker.positiveString(string);
		
		if(result) {
			System.out.println("It is a positive string");
		}
		else {
			System.out.println("It is not a positive string");
		}
		
		

	}

}
