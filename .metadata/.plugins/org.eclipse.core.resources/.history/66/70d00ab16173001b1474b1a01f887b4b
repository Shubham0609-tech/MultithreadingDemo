package com.cg.labs.exercise3;

import java.util.StringTokenizer;

public class AlterString {
	
	char c;
	
	void alterString(String s) {
		
		for(int i=0;i<s.length();i++) {
			
			c = s.charAt(i);
			if(c == 'a'||c=='A'||c == 'e'||c=='E') {
				
				System.out.println(s.charAt(i)+1);
				
			}
			else {
				System.out.println(s.charAt(i));
			}
					
			
		}
			
	}

}
