package com.cg.labs.exercise6;

public class PositiveString {
	
	public boolean positiveString(String s) {
		
		int flag = 0;
		
		for(int i=1;i<s.length();i++) {
			
			if(s.charAt(i-1) < s.charAt(i)) {
				
				flag = flag +1;				
			}			
		}
		
		if(flag == (s.length()-1)) {
			return true;
		}
		
		else {
			return false;
		}
		
	}

}
