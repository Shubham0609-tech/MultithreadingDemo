package com.cg.labs.exercise3;

public abstract class WrittenItem extends Item {
	
	protected String author;

	public WrittenItem(int uid, String tiles, int noOfCopies, String author) {
		super(uid, tiles, noOfCopies);
		this.author = author;
	}

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}
	
	public WrittenItem() {
		
	}
	

}
